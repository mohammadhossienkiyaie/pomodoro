{
  "name": "focusplus",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "electron": "^28.0.0"
  },
  "devDependencies": {
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.example.focusplus",
    "win": {
      "target": "nsis",
      "icon": "icons/appIcon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "icons/appIcon.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "icons/appIcon.png"
    }
  }
}
